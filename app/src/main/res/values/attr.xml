<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimeView">
        <attr name="color" format="color" />
        <attr name="emptyColor" format="color" />
        <attr name="itemMargin" format="dimension" />
        <attr name="showType" format="integer">
            <enum name="year" value="0" />
            <enum name="month" value="1" />
            <enum name="day" value="2" />
            <enum name="hour" value="3" />
            <enum name="min" value="4" />
            <enum name="sec" value="5" />
            <enum name="msec" value="6" />
        </attr>
    </declare-styleable>
    <declare-styleable name="RangeSliderView">
        <attr name="rangeCount" format="integer" />
        <attr name="filledColor" format="color" />
        <attr name="emptyColors" format="color" />
        <attr name="barHeightPercent" format="float" />
        <attr name="slotRadiusPercent" format="float" />
        <attr name="sliderRadiusPercent" format="float" />
    </declare-styleable>

    <declare-styleable name="RangeSeekBar">
        <attr name="cirCount" format="integer" />
        <attr name="cirRadius" format="dimension" />
        <attr name="primaryColor" format="color" />
        <attr name="normalColor" format="color" />
        <attr name="seekHeight" format="dimension" />
    </declare-styleable>

</resources>